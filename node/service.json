    "generateSchema": {
      "path": "/returns/generateSchema",
      "public": true,
      "settingsType": "workspace"
    },
    "getDocuments": {
      "path": "/returns/getDocuments/:schemaName/:type/:whereClause",
      "public": true,
      "settingsType": "workspace"
    },
    "saveDocuments": {
      "path": "/returns/saveDocuments/:schemaName",
      "public": true,
      "settingsType": "workspace"
    },
    "savePartialDocument": {
      "path": "/returns/savePartialDocument/:schemaName",
      "public": true,
      "settingsType": "workspace"
    },
    "getCategories": {
      "path": "/returns/getCategories/",
      "public": true,
      "settingsType": "workspace"
    },
    "createGiftCard": {
      "path": "/returns/giftCard/",
      "public": true,
      "settingsType": "workspace"
