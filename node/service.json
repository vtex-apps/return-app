{
  "memory": 512,
  "ttl": 60,
  "timeout": 5000,
  "minReplicas": 2,
  "maxReplicas": 4,
  "routes": {
    "returnRequests": {
      "path": "/_v/return-request",
      "public": true
    },
    "returnRequest": {
      "path": "/_v/return-request/:requestId",
      "public": true
    },
    "_returnRequests": {
      "path": "/_v/return-request",
      "public": false,
      "policies": [
        {
          "effect": "allow",
          "actions": ["get"],
          "principals": [
            "vrn:apps:*:*:*:app/vtex.return-app@*"
          ]
        }
      ]
    }
  }
}
