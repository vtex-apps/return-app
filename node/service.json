    "apiUpdateStatus": {
      "path": "/no-cache/returns/api/update-status/:request_id",
      "public": true
    },
    "apiCreateRequest": {
      "path": "/no-cache/returns/api/create-request",
      "public": false,
      "access": "authorized",
      "policies": [{
        "effect": "allow",
        "actions": ["post"],
        "principals": [
          "vrn:apps:*:*:*:app/elefantdev.rma-proxy@*"
        ]
      }]
    }
  }
}
