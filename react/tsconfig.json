{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2017",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "jsx": "preserve",
    "lib": [
      "es2019",
      "DOM"
    ],
    "noEmitOnError": false,
    "sourceMap": true,
    "noImplicitAny": false,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "node",
      "jest",
      "graphql"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./typings/*.d.ts",
    "./**/*.tsx",
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.ts",
    "./**/*.d.ts"
  ],
  "typeAcquisition": {
    "enable": false
  },
  "extends": "@vtex/tsconfig"
}